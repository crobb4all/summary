									"listen": "prerequest",
									"script": {
										"exec": [
											"const currentDate = new Date();\r",
											"\r",
											"// Функция для форматирования даты и времени в нужном формате\r",
											"const formatDate = (date) => {\r",
											"  const year = date.getFullYear();\r",
											"  const month = String(date.getMonth() + 1).padStart(2, '0');\r",
											"  const day = String(date.getDate()).padStart(2, '0');\r",
											"  const hours = String(date.getHours()).padStart(2, '0');\r",
											"  const minutes = String(date.getMinutes()).padStart(2, '0');\r",
											"  const seconds = String(date.getSeconds()).padStart(2, '0');\r",
											"  const offset = String(-(date.getTimezoneOffset() / 60)).padStart(2, '0');\r",
											"\r",
											"  return `${year}-${month}-${day}T${hours}:${minutes}:${seconds}+${offset}:00`;\r",
											"};\r",
											"\r",
											"// Генерируем дату и время начала события\r",
											"const startAt = formatDate(currentDate);\r",
											"\r",
											"// Генерируем дату и время конца события с сдвигом на 1 час\r",
											"const endAt = formatDate(new Date(currentDate.getTime() + 60 * 60000)); // Добавляем 60 минут к текущему времени\r",
											"\r",
											"// Создаем объект запроса с актуальными датой и временем\r",
											"const requestBody = {\r",
											"  startAt: startAt,\r",
											"  endAt: endAt,\r",
											"  title: \"ТестиРОваниеAPi\",\r",
											"  color: \"#81888D\",\r",
											"  backgroundColor: \"#FFF7C7\"\r",
											"};\r",
											"\r",
											"// Подставляем актуальные дату и время в тело запроса\r",
											"pm.request.body.raw = JSON.stringify(requestBody);"
										],
